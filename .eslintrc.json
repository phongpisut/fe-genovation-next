{
    "env": {
      "browser": true,
      "es2021": true
    },
    "extends": [
      "eslint:recommended",
      "plugin:@next/next/recommended",
      "plugin:prettier/recommended"
    ],
    "parserOptions": {
      "project": "./tsconfig.json"
    },
    
    "rules": {
      "prettier/prettier": ["error", { "endOfLine": "auto" }],
      "react/react-in-jsx-scope": "off"
    }
  }