{
    "semi": true,
    "singleQuote": true,
    "printWidth": 80,
    "trailingComma": "es5",
    "tabWidth": 2,
    "bracketSpacing": true,
    "jsxBracketSameLine": true,
    "arrowParens": "always",
    "proseWrap": "preserve",
    "endOfLine": "auto",
    "overrides": [
      {
        "files": ["*.ts", "*.tsx"],
        "options": {
          "parser": "typescript"
        }
      }
    ],
    "plugins": ["prettier-plugin-tailwindcss", "prettier-plugin-organize-imports"]
    
}